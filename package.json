{
  "version": "0.1.0",
  "license": "MIT",
  "name": "myreact",
  "author": "MrWangJustToDo",
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "dev:ssr": "cd ./ui/ssr-example && yarn run dev",
    "dev:csr": "cd ./ui/csr-example && yarn run dev",
    "dev:next": "cd ./ui/next-example && yarn run dev",
    "dev:vite": "cd ./ui/vite-example && yarn run dev",
    "dev:vite-ssr": "cd ./ui/vite-ssr-example && yarn run dev",
    "start:ssr": "cd ./ui/ssr-example && yarn run build && yarn run start",
    "build:site": "cd ./ui/ssr-example && yarn run build:static",
    "gen:gql": "graphql-codegen --config site/graphql/codegen.yaml",
    "dev": "ts-node ./scripts/rollupWatch.ts",
    "build": "ts-node ./scripts/rollupBuild.ts",
    "release": "ts-node ./scripts/release.ts",
    "lint": "eslint --cache --ext ts,tsx .",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier --ignore-path .prettierignore --write .",
    "clean": "yarn -p -r -c exec rm -rf dist dev .cache",
    "purge": "yarn -p -r -c exec rm -rf node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build"
    }
  },
  "devDependencies": {
    "@apollo/client": "^3.9.5",
    "@babel/runtime": "^7.22.5",
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.6",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@my-react/react": "workspace:*",
    "@my-react/react-dom": "workspace:*",
    "@my-react/react-jsx": "workspace:*",
    "@my-react/react-reactive": "workspace:*",
    "@my-react/react-reconciler": "workspace:*",
    "@my-react/react-refresh": "workspace:*",
    "@my-react/react-refresh-tools": "workspace:*",
    "@my-react/react-shared": "workspace:*",
    "@swc/core": "^1.4.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "cross-fetch": "^4.0.0",
    "cross-port-killer": "^1.4.0",
    "eslint": "^8.57.0",
    "graphql": "^16.8.0",
    "husky": "^9.0.11",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier": "^3.2.5",
    "project-tool": "https://github.com/MrWangJustToDo/project-tool.git",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
